{
	"name": "gvhuyssteen/laravel-vimeo",
	"description": "Laravel 4.2 wrapper for the official Vimeo API. Original: https://github.com/vinkla/vimeo",
	"homepage": "https://github.com/gvhuyssteen/laravel-vimeo",
	"license": "MIT",
	"keywords": ["laravel", "vimeo", "video"],
	"authors": [
		{
			"name": "Vincent Klaiber (Owner)",
			"email": "me@vinkla.com",
			"homepage": "http://vinkla.com"
		},
        {
          "name": "Gerrit van Huyssteen (Contributor)",
          "email": "gerrit@brazenhill.com",
          "homepage": "http://www.brazenhill.com"
        }
	],
	"require": {
		"php": ">=5.4.0",
		"vimeo/vimeo-api": "~1.0",
		"illuminate/support": "~4.0|~5.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0"
	},
	"autoload": {
		"psr-0": {
			"Vinkla\\Vimeo": "src/"
		}
	},
	"minimum-stability": "stable"
}
